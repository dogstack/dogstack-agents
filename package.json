{
  "name": "dogstack-agents",
  "version": "0.1.2",
  "description": "dogstack plugin to provide core user authentication, profiles, and relationships",
  "main": "index.js",
  "browser": "browser.js",
  "scripts": {
    "postinstall": "./bin/link-db-migrations link",
    "postuninstall": "./bin/link-db-migrations unlink",
    "start": "node-dev example",
    "test:deps": "dependency-check . --detective precinct && dependency-check . --extra --no-dev --detective precinct -i babelify -i babel-preset-es2015 -i babel-preset-react -i babel-plugin-ramda",
    "test:lint": "standard",
    "test:node": "NODE_ENV=test run-default tape test/*.js --",
    "test:coverage": "NODE_ENV=test nyc npm run test:node",
    "test:coverage:report": "nyc report --reporter=lcov npm run test:node",
    "test": "npm-run-all -s test:node test:lint test:deps"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "ramda"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dogstack/dogstack-agents.git"
  },
  "keywords": [],
  "author": "Root Systems <hello@rootsystems.nz>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dogstack/dogstack-agents/issues"
  },
  "homepage": "https://github.com/dogstack/dogstack-agents#readme",
  "devDependencies": {
    "dependency-check": "^2.7.0",
    "node-dev": "^3.1.3",
    "npm-run-all": "^4.0.1",
    "nyc": "^10.1.2",
    "precinct": "^3.6.0",
    "run-default": "^1.0.0",
    "standard": "^8.6.0",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "@f/create-action": "^1.1.1",
    "babel-plugin-ramda": "^1.2.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "feathers-action": "^2.2.0",
    "feathers-authentication": "^1.2.6",
    "feathers-authentication-client": "^0.3.2",
    "feathers-authentication-jwt": "^0.3.1",
    "feathers-authentication-local": "^0.3.4",
    "feathers-authentication-oauth2": "^0.2.5",
    "feathers-hooks-common": "ahdinosaur/feathers-hooks-common#build",
    "feathers-knex": "^2.6.3",
    "incremental-id": "^1.0.0",
    "passport-facebook": "^2.1.1",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^1.0.0",
    "ramda": "^0.24.0",
    "react": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-auth-wrapper": "github:ahdinosaur/redux-auth-wrapper#build-v2",
    "redux-fp": "^0.2.0",
    "redux-observable": "^0.14.1",
    "reselect": "^3.0.1",
    "run-parallel": "^1.1.6",
    "rxjs": "^5.4.0",
    "tcomb": "^3.2.20",
    "tcomb-form": "^0.9.16"
  }
}
